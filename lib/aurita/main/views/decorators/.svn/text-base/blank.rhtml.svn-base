</body>

<script language="javascript" type="text/javascript">

  window.onload = function() { Element.hide('cover'); Cuba.collapse_boxes();  }

  var recently_viewed = []; 
  var recently_viewed_titles = []; 
  add_recently_viewed = function(asset_id, title) { 
    recently_viewed_titles[asset_id] = title; 
    if(recently_viewed.indexOf(asset_id) != -1) { 
      // remove previous appearance of asset_id from list: 
      recently_viewed.splice(recently_viewed.indexOf(asset_id), 1); 
    }
    else { 
      // remove last asset_id from list: 
      if(recently_viewed.length > 10) recently_viewed.shift(); 
    }
    recently_viewed.push(asset_id); 
    template = $('recently_viewed_element_template').innerHTML; 
    content = ''; 
    recently_viewed.reverse(); 
    for(i=0; i<recently_viewed.length; i++) {
      aid = recently_viewed[i]; 
      title = recently_viewed_titles[aid]; 
      content += template.replace('__id__', aid).replace('__id__', aid).replace('__id__', aid).replace('{title}', title); 
    }
    recently_viewed.reverse(); 
    $('recently_viewed_list').innerHTML = content; 
  }
  active_button = document.getElementById('button_App_General'); 
  if(active_button) { 
    active_button.className = 'header_button_active';
  }

</script>


