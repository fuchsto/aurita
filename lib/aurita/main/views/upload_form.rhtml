<% 
  form.enctype  = 'multipart/form-data'
  form.method   = 'POST'
  form.target   = 'asset_upload_frame' unless form.target
# form.onsubmit = 'Aurita.submit_upload_form(this); return false;' unless form.onsubmit
%>

<div class="form_box">
  
  <div id="form_content">
    <%= form.string %>
  </div>

  <div class="form_button_bar" id="<%= form.dom_id %>_buttons">
    <%= gui.button(:class => :submit, :type => :submit, :icon => :ok, 
                   :onclick => js.Aurita.submit_upload_form(form.dom_id)) { tl(:ok) } %>
    <%= gui.button(:class => :cancel, :icon => :cancel, 
                   :onclick => js.Aurita.cancel_form(form.dom_id)) { tl(:cancel) } %>
  </div>

</div>
